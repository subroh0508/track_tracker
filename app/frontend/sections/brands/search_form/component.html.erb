<%= form_with url: url, method: :get, data: data, class: classes do |_| %>
  <%= content %>
  <%= render Form::LabelComponent.new(
    label_for: "query",
    classes: "block",
  ) do %>
    アルバム名
  <% end %>
  <%= render Input::Component.new(
    type: "search",
    id: "query",
    placeholder: "Search albums by title",
    icon: "fas fa-magnifying-glass",
    button_label: "Go",
    value: query,
  ) %>
<% end %>
